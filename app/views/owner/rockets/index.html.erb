<h1>My rockets</h1>

<% @rockets.each do |rocket| %>
  <div class='card p-3 m-3'>
    <div>
      <p><%= rocket.name %></p>
      <p><%= rocket.daily_price %></p>
      <p><%= rocket.location %></p>
      <p><%= rocket.autonomy %></p>
    </div>
    <div>
      <%= link_to "#" do %>
        <button type="button" class="btn btn-light">Edit</button>
      <% end %>
      <%= link_to "#" do %>
        <button type="button" class="btn btn-danger">Delete</button>
      <% end %>
    </div>
  </div>
<% end %>
